<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx/11.0.1" 
      xmlns:fx="http://javafx.com/fxml/1" 
      fx:controller="com.loginsystem.SettingsController"
      prefHeight="400.0" prefWidth="300.0">
   
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="设置" textAlignment="CENTER">
      <font>
         <Font name="System Bold" size="24.0" />
      </font>
   </Text>
   
   <Text text="这是设置页面" />
   <Text text="后续可以在这里添加各种设置选项" />
   
   <Button mnemonicParsing="false" onAction="#handleClose" text="关闭" />
</VBox>